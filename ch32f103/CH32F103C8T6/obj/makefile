# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
CH32F103C8T6.hex \

SECONDARY_LIST += \
CH32F103C8T6.lst \

SECONDARY_SIZE += \
CH32F103C8T6.siz \


# 所有目标
all: CH32F103C8T6.elf secondary-outputs

# 工具调用
CH32F103C8T6.elf: $(OBJS) $(USER_OBJS)
	@	@	arm-none-eabi-gcc -mcpu=cortex-m3 -mthumb -mthumb-interwork -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common  -g -T "D:\stm32_code\ch32f103\CH32F103C8T6\LD\CH32F103C8T6.ld" -nostartfiles -Xlinker --gc-sections -Wl,-Map,"CH32F103C8T6.map" --specs=nano.specs --specs=nosys.specs -o "CH32F103C8T6.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@	@
CH32F103C8T6.hex: CH32F103C8T6.elf
	@	arm-none-eabi-objcopy -O ihex "CH32F103C8T6.elf"  "CH32F103C8T6.hex"
	@	@
CH32F103C8T6.lst: CH32F103C8T6.elf
	@	arm-none-eabi-objdump --source --all-headers --demangle --disassemble "CH32F103C8T6.elf" > "CH32F103C8T6.lst"
	@	@
CH32F103C8T6.siz: CH32F103C8T6.elf
	@	arm-none-eabi-size --format=berkeley "CH32F103C8T6.elf"
	@	@
# 其他目标
clean:
	-$(RM) $(ASM_UPPER_DEPS)$(OBJS)$(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_DEPS)$(S_UPPER_DEPS)$(C_DEPS) CH32F103C8T6.elf
	-@
secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

